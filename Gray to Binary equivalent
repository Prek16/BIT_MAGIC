//User function Template for Java

class Solution{
    
    // function to convert a given Gray equivalent n to Binary equivalent.
    public static int grayToBinary(int n) {
        
        // Your code here
        int a=n;
        if(n==0)return 0;
        while(n>0)
        {
            a=a^(n>>1);
            n=n>>1;
        }
        return a;
    }
       
}
