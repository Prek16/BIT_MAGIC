class Solution
{
    //Function to find position of first set bit in the given number.
    public static int getFirstSetBit(int n){
            
        int c=0;
        if(n==0)
        return 0;
        int i;
        for(i=0;i<32;i++)
        {
            if((n&(1<<i))!=0)
            break;
        }
            return i+1;
    }
}
